{
  "name": "express-typescript-template",
  "version": "1.0.0",
  "description": "backend template with typescript,typeorm,express",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest --coverage",
    "start": "node dist/index.js",
    "build": "tsc",
    "watch": "ts-node ./src/index.ts",
    "nodemon": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/index.ts",
    "lint": "eslint --fix",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate --name",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:down": "ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siner308/express-typescript-template.git"
  },
  "keywords": [
    "template",
    "express",
    "typescript",
    "typeorm"
  ],
  "author": "siner308",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/siner308/express-typescript-template/issues"
  },
  "homepage": "https://github.com/siner308/express-typescript-template#readme",
  "dependencies": {
    "@sentry/node": "^5.29.0",
    "@sentry/tracing": "^5.29.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.9",
    "@types/express-session": "^1.17.3",
    "@types/lodash": "^4.14.165",
    "@types/multer": "^1.4.5",
    "aws-sdk": "^2.809.0",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "class-validator-jsonschema": "^2.0.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "lodash": "^4.17.20",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "routing-controllers": "^0.9.0-alpha.6",
    "routing-controllers-openapi": "^2.2.0",
    "ts-node": "^9.1.1",
    "typeorm": "^0.2.29",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-unused-imports": "^1.0.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "supertest": "^6.0.1",
    "swagger-ui-express": "^4.1.5",
    "ts-jest": "^26.4.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": "\\.spec.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
